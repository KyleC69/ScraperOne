<reactiveUi:ReactiveWindow x:TypeArguments="viewModels:ManagerViewModel"
                           x:Class="ScraperOne.Views.ShellWin"
                           xmlns="https://github.com/avaloniaui"
                           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                           xmlns:views="clr-namespace:ScraperOne.Views"
                           xmlns:viewModels="clr-namespace:ScraperOne.ViewModels"
                           x:CompileBindings="true" x:DataType="viewModels:ManagerViewModel"
                           xmlns:reactiveUi="http://reactiveui.net"
                           mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                           Title="Scraper One Manager" Background="Black"
                           Width="1150" Height="800" >

    <Grid ColumnDefinitions="*,*,*" RowDefinitions="48,750,50" >

        <views:ButtCommands Grid.Column="1" Margin="4" Height="45" Width="1150" Grid.Row="0" />
        <views:BlogManagerView Grid.Column="1" Grid.Row="1" Width="1150" Height="750" />
<views:Footer Grid.Column="1" Grid.Row="2"/>
    </Grid>
</reactiveUi:ReactiveWindow>